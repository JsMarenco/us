---
// Third-party dependencies
import type { DeYoPaTu, User } from "@prisma/client";

// Current project dependencies
import Deyopatus from "./Deyopatus.astro";
import UserProfileHeader from "../../Forms/EditUser/UserProfileHeader";

type Props = {
  user: User;
  createdDeYoPaTus: DeYoPaTu[];
};

const { user, createdDeYoPaTus } = Astro.props;
---

<UserProfileHeader client:load user={user} />
<Deyopatus
  createdDeYoPaTus={createdDeYoPaTus}
  firstName={user.firstName}
  lastName={user.lastName}
  username={user.username}
/>
